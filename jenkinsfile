/*import library*/
@Library('kartiklibrary') _

pipeline {
    agent any
    stages{
           stage('build') {
              steps {
                   script { 
                       build()
                       echo "or Maven.mvn 'package' (using class in src)"
              }
            }
          }
          stage('notify'){
           steps {
             script {
               notification.slack(currentBuild.currentResult)
               notification.email(currentBuild.currentResult)
            }
           }
          }
          
}          
