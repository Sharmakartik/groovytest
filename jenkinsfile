@Library('kartiklibrary') _

pipeline {
    agent any
    stages{
           stage('build') {
              steps {
                   script { 
                       build()
              }
            }
          }
          stage('notify'){
           steps{
             script{
               notification.slack(currentBuild.currentResult)
               notification.email(currentBuild.currentResult)
            }
           }
          }
          
}          
